    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-plugin-autofix": "^1.1.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.4"
